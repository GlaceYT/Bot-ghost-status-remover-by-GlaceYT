{
  "name": "hash-arg",
  "version": "1.0.7",
  "description": "CLI parameter parser to get the named and typed value",
  "repository": {
    "type": "git",
    "url": "https://github.com/takamin/hash-arg"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/index.js",
  "engines": {
    "node": ">=8.17.0",
    "npm": ">=6.13.4"
  },
  "scripts": {
