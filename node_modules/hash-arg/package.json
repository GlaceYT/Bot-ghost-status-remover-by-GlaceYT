  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.18.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-watch": "^8.0.0",
    "mocha": "^10.0.0",
    "node-getopt": "github:takamin/node-getopt",
    "nyc": "^15.1.0"
  }
